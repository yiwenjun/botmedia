# BotMedia - .env Template
# Copy this file to .env and fill in your actual values

# ==================== Database ====================
MYSQL_PASSWORD=your_secure_password_here

# ==================== JWT ====================
JWT_SECRET=BotMedia2026SecretKeyForJwtTokenGeneration256Bits!

# ==================== RabbitMQ ====================
RABBITMQ_USERNAME=guest
RABBITMQ_PASSWORD=guest

# ==================== WeChat Official Account ====================
WECHAT_APP_ID=your_wechat_app_id
WECHAT_APP_SECRET=your_wechat_app_secret
WECHAT_TOKEN=your_wechat_token
WECHAT_ENCODING_AES_KEY=your_encoding_aes_key

# ==================== WeChat Mini Program ====================
WECHAT_MINIAPP_APP_ID=your_miniapp_app_id
WECHAT_MINIAPP_APP_SECRET=your_miniapp_app_secret

# ==================== WeChat Pay ====================
WECHAT_PAY_MCH_ID=your_merchant_id
WECHAT_PAY_API_KEY=your_api_key
WECHAT_PAY_CERT_PATH=/certs/apiclient_cert.p12
WECHAT_PAY_NOTIFY_URL=https://api.botmedia.com/api/v1/payments/wechat/callback

# ==================== Alibaba Cloud OSS ====================
ALIYUN_OSS_ENDPOINT=oss-cn-hangzhou.aliyuncs.com
ALIYUN_OSS_BUCKET=botmedia-media
ALIYUN_OSS_CDN_DOMAIN=cdn.botmedia.com
ALIYUN_ACCESS_KEY_ID=your_access_key_id
ALIYUN_ACCESS_KEY_SECRET=your_access_key_secret

# ==================== Alibaba Cloud ACR (Container Registry) ====================
ACR_USERNAME=your_acr_username
ACR_PASSWORD=your_acr_password

# ==================== ECS Deployment ====================
ECS_HOST=your_ecs_public_ip
ECS_USER=deploy
